.con7 h2{
    font-size: 50px;
    margin: 20px 0px;
    font-family: 'TTWanjudaedunsancheB';
}
.con7 h2 span{
    font-size: 35px;
}
.con7 img{
    display:block;
    width:55vw;
    -webkit-mask-image: url("../img/asset.png");
    -webkit-mask-repeat: no-repeat;
    -webkit-mask-size: 100% 100%;
    mask-image: url("../img/asset.png");
    mask-repeat: no-repeat;
     mask-size: 100% 100%;
}
.con7 ul{display:flex;width:70%; padding:1%;transform: translate(-20%,-70%); }
.ticket_title{font-size: 2vw; transform: translate(-20%,-150%);width: 60%;font-family: 'MICEGothic bold'}
.con7 ul >  li{font-size:1vw;width: 40%;}
.con7 ul >  li input{
    text-align: center;
    background: #eee;
    margin: 8px 0;
    width: 100%;
    border: 0;
    outline: none;
    border-radius: 20px;
    box-shadow: inset 7px 2px 10px #babebc, inset -5px -5px 12px #fff;}
.con7 ul >  li:nth-of-type(1) input{width:50%;padding:1%;font-size:1vw;}
.con7 ul >  li:nth-of-type(2) input{width:50%;padding:1%;font-size:1vw;}
.con7 .ticket_wrap{position:absolute;top:50%; left: 24%; height: 30vw;}
.con7 .ticket_title, li{color:#fff;}
.con7 .ticket_wrap h2{position: absolute;top:-8%;left: 22vw;font-size: 8vw;}
#ticket_button{ 
    border-radius: 20px;
    border: none;
    outline: none;
    font-size: 1vw;
    font-weight: bold;
    padding:1%;
    letter-spacing: 1px;
    text-transform: uppercase;
    cursor: pointer;
    transition: transform 80ms ease-in;border: 0;font-size:1vw;}

.con7_left_text{
    font-size:1vw;
    position:absolute;
    top:30%;
    left: 75%;
    width:30%;
    transform:translateY(-110%);
    opacity:0;
    line-height:1.5vw;
    /*color:#f6a3a5;*/
}


.con7:hover .con7_left_text{transition:1s; transform:translateY(0%); opacity:1;}


}



